# name: test/optimizer/pushdown/timestamp_to_date_pushdown.test
# description: Test Tpushdown of timestamp to date filters
# group: [pushdown]

require icu

statement ok
set TimeZone='UTC';

statement ok
create table t1 (ts timestamp, i int);

statement ok
insert into t1 select '2024-05-01 00:00:00'::timestamp, i from generate_series(1, 122880) g(i);

statement ok
insert into t1 select '2024-05-02 00:00:00'::timestamp, i from generate_series(1, (122880/2)::INT) g(i);

statement ok
insert into t1 select '2024-05-02 00:24:00'::timestamp, i from generate_series(1, (122880/2)::INT) g(i);

statement ok
insert into t1 select '2024-05-03 00:00:00'::timestamp, i from generate_series(1, 122880) g(i);

query II
explain select * from t1 where ts::date == '2024-05-02';
----
physical_plan	<REGEX>:.*SEQ_SCAN.*Filters:.*

query II
explain select * from t1 where '2024-05-02' == ts::date;
----
physical_plan	<REGEX>:.*SEQ_SCAN.*Filters:.*


statement ok
pragma disable_optimizer;

query II nosort no_opt_result
select * from t1 where ts::date == '2024-05-02';
----


statement ok
pragma enable_optimizer;

query II nosort no_opt_result
select * from t1 where ts::date == '2024-05-02';
----
