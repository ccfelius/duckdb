# name: test/optimizer/joins/setops_retain_original_binding_order.test
# description: Set operations need to retain their original binding order on the left and right sides.
# group: [joins]

statement ok
create table t1 as select range::INT a, range::FLOAT b, range::VARCHAR c from range(10000);


statement ok
create table t2 as select range::INT a from range (10);

statement ok
create table t3 as select range::FLOAT b, range::VARCHAR c from range(100);

# if the join order optimizer switches the corss product of t2/t3, the order of the columns changes
# the join order optimizer then needs to add a projection to fix this. 
statement ok
select * from t1 UNION BY NAME (select * from t2, t3);
