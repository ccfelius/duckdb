WITH RECURSIVE t(x, y) AS
(
  SELECT 1, 1
    UNION ALL
  SELECT 1, 2
    UNION ALL
  (SELECT x+1, 1
  FROM   t
  WHERE  x < 10 AND y = 1
    UNION ALL
  SELECT x+1, 2
  FROM   t
