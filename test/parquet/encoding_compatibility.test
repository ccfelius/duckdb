# name: test/parquet/encoding_compatibility.test
# description: Test Parquet writer encoding_compatibility parameter
# group: [parquet]

require parquet

statement ok
pragma enable_verification

statement error
copy (select range i from range(20000)) to '__TEST_DIR__/encoding_compatibility.parquet' (encoding_compatibility)
----
Binder Error

statement error
copy (select range i from range(20000)) to '__TEST_DIR__/encoding_compatibility.parquet' (encoding_compatibility v3)
----
Binder Error

# we do PLAIN if we can't do dictionary for V1
statement ok
copy (select range i from range(20000)) to '__TEST_DIR__/encoding_compatibility.parquet' (encoding_compatibility v1)

query I
select encodings from parquet_metadata('__TEST_DIR__/encoding_compatibility.parquet')
----
PLAIN

# we do DELTA_BINARY_PACKED for V2
statement ok
copy (select range i from range(20000)) to '__TEST_DIR__/encoding_compatibility.parquet' (encoding_compatibility v2)

query I
select encodings from parquet_metadata('__TEST_DIR__/encoding_compatibility.parquet')
----
DELTA_BINARY_PACKED
