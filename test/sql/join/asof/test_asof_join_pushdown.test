CREATE OR REPLACE TABLE right_pushdown(time INTEGER, value FLOAT);

statement ok
INSERT INTO right_pushdown VALUES
    (0, 0), 
    (1, NULL), 
;

query IIII
SELECT 
	d1.time,
	d2.time,
	d1.value, 
	d2.value
FROM right_pushdown d1 
ASOF JOIN (
	SELECT * FROM right_pushdown WHERE value is not NULL
	) d2
	ON d1.time >= d2.time;
----
0 	0	0.0	0.0
1 	0	NULL	0.0

query IIII
