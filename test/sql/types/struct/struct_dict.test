# name: test/sql/types/struct/struct_dict.test
# description: Test dict syntax for structs
# group: [struct]

query I
SELECT {'i': 1, 'j': 2};
----
{'i': 1, 'j': 2}

query I
SELECT {'i': NULL, 'j': 2};
----
{'i': NULL, 'j': 2}

query I
SELECT {'i': [], 'j': 2};
----
{'i': [], 'j': 2}

query I
SELECT {'i': [1, 2, 3], 'j': 2};
----
{'i': [1, 2, 3], 'j': 2}

# duplicate key
statement error
SELECT {'i': 3, 'i': 4}

# non-constant key
statement error
SELECT {i: 3} FROM range(100) tbl(i)

# empty struct is not allowed
statement error
SELECT {};

# need string keys
statement error
SELECT {1:3};
