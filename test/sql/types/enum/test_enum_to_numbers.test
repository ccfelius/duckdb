# name: test/sql/types/enum/test_enum_to_numbers.test
# description: ENUM types used in table tests
# group: [enum]

statement ok
create type enum1 as enum ('1', '2', '3', '4');

statement ok
create table t1 as select range as a from range(10);

statement ok
create table t2 (a enum1);

statement ok
