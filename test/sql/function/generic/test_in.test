----
false

statement error
SELECT 1 IN [];
----
IN must contain at least 1 item

statement ok
PREPARE v1 AS SELECT 1 IN ?

query I
EXECUTE v1([1, 2, 3]);
----
true

query I
EXECUTE v1([2, 2, 3]);
----
false

query I
EXECUTE v1((1, 2, 3));
----
true

query I
EXECUTE v1((2, 2, 3));
----
false

statement error
EXECUTE v1([]);
----
Expanded LIST parameter inside IN expression can not be empty

query I
EXECUTE v1(MAP {1: 'a', 2: 'b', 3: 'c'});
----
true

query I
EXECUTE v1(MAP {2: 'b', 3: 'c'});
----
false

query I
SELECT 1 IN (MAP {1: 'a', 2: 'b', 3: 'c'});
----
