# Structs

query I
select list_resize([{'i': 1,'j': 2}, NULL, {'i': 3, 'j': 4}], 2);
----
[{'i': 1, 'j': 2}, NULL]

query I
select list_resize([{'i': 1,'j': [2, 3]}, NULL, {'i': 1, 'j': [2, 3]}], 4);
----
[{'i': 1, 'j': [2, 3]}, NULL, {'i': 1, 'j': [2, 3]}, NULL]

# Nested Function

query I
