# name: test/sql/copy/csv/test_partition_gz.test
# description: Test we can round-trip partitioned gzipped CSV files
# group: [csv]

statement ok
PRAGMA enable_verification

statement ok
CREATE TABLE test AS VALUES ('a', 'foo', 1), ('a', 'foo', 2), ('a', 'bar', 1), ('b', 'bar', 1);

statement ok
COPY (FROM test) TO 'data.csv.d' (FORMAT 'csv', COMPRESSION 'gzip', PARTITION_BY ('col0', 'col1'));

# Specify Compression
query II
FROM read_csv_auto('data.csv.d/*/*/*.csv', compression='gzip')
----
a	bar	1
a	foo	1
a	foo	2
b	bar	1

