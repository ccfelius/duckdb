# name: test/sql/copy/csv/test_blob.test
# description: Test BLOB with COPY INTO
# group: [csv]

# CSV file with almost ALL ASCII chars
# ASCII chars from 1 to 255, skipping '
# COPY INTO
statement ok
CREATE TABLE blobs (b BYTEA);

query I
COPY blobs FROM 'test/sql/copy/csv/data/test/blob.csv';
----
1

# test if the system correctly loads/stores the bytes
query T
SELECT b FROM blobs
----
\

# testing copying to a file and copying them back in
query I
COPY blobs TO '__TEST_DIR__/blob.csv';
----
1

statement ok
DELETE FROM blobs

query I
COPY blobs FROM '__TEST_DIR__/blob.csv';
----
1

query T
SELECT b FROM blobs
----
\

# now test it with a delimiter that occurs IN the blob
query I
COPY blobs TO '__TEST_DIR__/blob.csv' DELIMITER 'A' QUOTE 'B' ESCAPE 'C';
----
1

statement ok
DELETE FROM blobs

query I
COPY blobs FROM '__TEST_DIR__/blob.csv' DELIMITER 'A' QUOTE 'B' ESCAPE 'C';
----
1

query T
