# Test Copy To
statement ok
create table t (a integer);

statement ok
insert into t values (0), (null)

statement ok
COPY t TO '__TEST_DIR__/t_i.csv' (nullstr 'none')

# Not accepted in copy to/from
statement error
COPY t TO '__TEST_DIR__/t_i.csv' (nullstr ['none', ''])
----
syntax error at or near "["

statement error
COPY t FROM '__TEST_DIR__/t_i.csv' (nullstr ['none', ''])
----
syntax error at or near "["

statement ok
COPY t FROM '__TEST_DIR__/t_i.csv' (nullstr 'none')

