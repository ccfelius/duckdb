statement ok
create or replace table tbl as select * FROM ( VALUES
	([repeat('a', 1000000)]),
	([repeat('a', 1000000)]),
	([repeat('a', 1000000)]),
	([repeat('a', 1000000)]),
	([repeat('b', 1000000)]),
	([repeat('b', 1000000)]),
	([repeat('b', 1000000)]),
	([repeat('b', 1000000)]),
) tbl(i)

query I
select (min(distinct i)::TEXT)[2:2] from tbl;
----
a
