# simple overloaded macro
statement ok
CREATE MACRO multi_add
	(a) AS a,
	(a, b) AS a + b,
	(a, b, c) AS  a + b + c,
	(a, b, c, d) AS  a + b + c + d,
	(a, b, c, d, e) AS  a + b + c + d + e

query IIIII
SELECT multi_add(42),
       multi_add(42, 1),
       multi_add(42, 1, 1),
       multi_add(42, 1, 1, 1),
       multi_add(42, 1, 1, 1, 1)
----
42	43	44	45	46
