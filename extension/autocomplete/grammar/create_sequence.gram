CreateSequenceStmt <- 'SEQUENCE'i IfNotExists? QualifiedName SequenceOption*

SequenceOption <-
	SeqSetCycle /
	SeqSetIncrement /
	SeqSetMinMax /
	SeqNoMinMax /
	SeqStartWith /
	SeqOwnedBy

SeqSetCycle <- 'NO'i? 'CYCLE'i
SeqSetIncrement <- 'INCREMENT'i 'BY'i? NumberLiteral
SeqSetMinMax <- SeqMinOrMax NumberLiteral
SeqNoMinMax <- 'NO'i SeqMinOrMax
SeqStartWith <- 'START'i 'WITH'i? NumberLiteral
SeqOwnedBy <- 'OWNED'i 'BY'i QualifiedName


SeqMinOrMax <- 'MINVALUE'i / 'MAXVALUE'i

