# name: benchmark/micro/cast/cast_stringlist_intlist.benchmark
# description: Cast stringlist values to intlist
# group: [cast]

name Cast VARCHAR[] -> INTEGER[]
group cast

load
CREATE TABLE stringlist_to_intlist AS SELECT [i]::VARCHAR[] v FROM range(0, 10000000) tbl(i);

run
SELECT MAX(CAST(v AS INTEGER[])) FROM stringlist_to_intlist

result I
[9999999]