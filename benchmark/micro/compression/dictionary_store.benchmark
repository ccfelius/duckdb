# name: benchmark/micro/compression/bitpacking_store.benchmark
# description: Storing 1GB of ints compressed at ~3x compression
# group: [compression]

name Bitpacking Insert
group aggregate
storage persistent
require_reinit

load
PRAGMA force_compression='dictionary';
DROP TABLE IF EXISTS integers;

run
CREATE TABLE test AS SELECT (100 + (i%2))::VARCHAR AS i FROM range(0, 10000000) tbl(i);
checkpoint;
