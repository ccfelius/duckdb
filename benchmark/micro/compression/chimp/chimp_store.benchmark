# name: benchmark/micro/compression/chimp/chimp_store.benchmark
# description: Scanning a large amount of doubles
# group: [chimp]

name Chimp Insert
group chimp
storage persistent
require_reinit

load
PRAGMA force_compression='uncompressed';
DROP TABLE IF EXISTS temperatures;
CREATE TABLE temperatures (
	temperature DOUBLE
);

run
INSERT INTO temperatures SELECT temp FROM 'benchmark/trainbenchmark/float_datasets/city_temperature.csv' t(temp), range(28);
checkpoint;
