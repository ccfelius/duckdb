# name: ${FILE_PATH}
# description: ${DESCRIPTION}
# group: [sf1]

name Q${QUERY_NUMBER_PADDED}
group tpch
subgroup sf1

require tpch

require parquet

cache tpch_sf1_parquet.duckdb

load
call dbgen(sf=1);
EXPORT DATABASE '${BENCHMARK_DIR}/tpch_sf1_parquet/' (FORMAT PARQUET);

run extension/tpch/dbgen/queries-parquet/q${QUERY_NUMBER_PADDED}.sql

result extension/tpch/dbgen/answers/sf1/q${QUERY_NUMBER_PADDED}.csv