# name: ${FILE_PATH}
# description: ${DESCRIPTION}
# group: [ingestion]

name Q${table_name}
group ingestion
subgroup tpch

require parquet

require tpch

cache tpch_sf1_parquet.duckdb

load benchmark/ingestion/tpch/load.sql

run
create table ${table_name}_ingested as select * from read_parquet('${BENCHMARK_DIR}/tpch_parquet/${table_name}.parquet');

cleanup
DROP TABLE ${table_name}_ingested;